{
  "name": "admin-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm exec umi dev",
    "build": "pnpm exec umi build",
    "build:ssr": "cross-env UMI_ENV=ssr pnpm exec umi build",
    "start": "pnpm exec umi preview",
    "start:ssr": "cross-env NODE_ENV=production ts-node server.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@ant-design/pro-components": "^2.4.4",
    "antd": "^5.4.0",
    "framer-motion": "^10.12.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "umi": "^4.0.70",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@umijs/plugins": "^4.0.70",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "history": "^5.3.0",
    "react-router-dom": "^6.30.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.4"
  }
}